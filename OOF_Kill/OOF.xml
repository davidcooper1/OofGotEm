<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\UI.xsd">
    <Frame name="OofKill_OptionsPanel" parent="UIParent">
        <Layers>
            <Layer>
                <FontString inherits="GameFontNormalLarge" justifyH="CENTER" parentKey="Title" text="OOF Kill">
                    <Anchors>
                        <Anchor point="TOP" y="-10"></Anchor>
                    </Anchors>
                </FontString>
                <FontString inherits="GameFontNormal" justifyH="LEFT" parentKey="SoundChannelLabel" text="Sound Channel">
                    <Anchors>
                        <Anchor point="TOP" relativeKey="$parent.Title" relativePoint="BOTTOM" y="-20"></Anchor>
                        <Anchor point="LEFT" x="20"></Anchor>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Button name="OofKill_SoundChannelDropdown" parentKey="SoundChannel" inherits="UIDropDownMenuTemplate">
                <Anchors>
                    <Anchor point="TOP" relativeKey="$parent.SoundChannelLabel" relativePoint="BOTTOM" y="-10"></Anchor>
                    <Anchor point="LEFT" relativeKey="$parent.SoundChannelLabel" x="-20"></Anchor>
                </Anchors>
            </Button>
        </Frames>
        <!--<Scripts>
            <OnLoad>
                OofKill_AddOptionsPanel(self);
            </OnLoad>
        </Scripts>-->
    </Frame>
</Ui>